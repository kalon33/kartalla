<IfModule mod_negotiation.c>
    Options +MultiViews

    AddLanguage en .en
    AddLanguage fi .fi
    LanguagePriority en fi
    ForceLanguagePriority Prefer Fallback

    DirectoryIndex index
</IfModule>

<IfModule mod_deflate.c>
    <FilesMatch "\.json$">
        SetOutputFilter DEFLATE
    </FilesMatch>
</IfModule>
